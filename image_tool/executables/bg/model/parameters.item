package algo

struct Median {
  scalar x: built_in.uint32 (.defaultValue=11)
  scalar y: built_in.uint32 (.defaultValue=11)
  scalar full : built_in.bool (.defaultValue=true)
}
struct PseudoMedian {
  scalar n: built_in.uint32 (.defaultValue=2)
  array filter_size: built_in.uint32[n] (.defaultValue=15)
  scalar t: built_in.float (.defaultValue=-0.1)
}
struct BackgroundDetectionParameter {
  scalar selector: built_in.uint32 (.defaultValue=0, .minValue=0, .maxValue=1)
  variant parameter : selector -> {
    0: Median
    1: PseudoMedian
  }
}

package algo

struct Median {
  scalar x: built_in.uint32
  scalar y: built_in.uint32
  scalar full : built_in.bool
}
struct PseudoMedian {
  scalar n: built_in.uint32
  array filter_size: built_in.uint32[n]
  scalar t: built_in.float
}
struct BackgroundDetectionParameter {
  scalar selector: built_in.uint32
  variant parameter : selector -> {
    0: Median
    1: PseudoMedian
  }
}
